.container-fluid 
    .calculator  
        margin 8% auto
        #bg
            width 330px
            height 400px
            background-color #000000
            border-radius 10px
            box-shadow: 0 10px 20px 0 rgba(0, 0, 0, 0.3)
            #console
                width 290px
                margin 30px 20px auto 20px
                color #ffffff
                font-size 28px
            button   
                border-radius 50px
                width 50px
                height 50px
                margin 3px 5px 3px 5px 
                font-size 19px  
                font-weight 600              
            .right.btn
                background-color #FB8D12
                color #ffffff
                font-weight 600
            .btn.lg 
                width 110px
    .footer a
        color #323231
        font-size 30px