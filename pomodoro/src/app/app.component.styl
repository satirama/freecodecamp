.container-fluid 
    background-color #A1E806
    color #ffffff
    .title
        h1  
            margin 4% auto 2% auto
            font-size 45px
        p  
            font-style italic
    .set
        h2
            margin 0 8px 0 8px
        .col-md-4
            margin 3% auto 3% auto
    .clock 
        margin 3% auto 3% auto
        .timer
            border 3px solid #ffffff
            border-radius 50%
            width 250px
            height 250px
    .footer
        margin 60px auto auto auto
        a i  
            color #ffffff