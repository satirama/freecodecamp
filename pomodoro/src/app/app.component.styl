.container-fluid 
    background-color #A1E806
    color #ffffff
    .title
        h1  
            margin 4% auto 2% auto
            font-size 45px
        p  
            font-style italic
    .set
        button
            margin 0 15px 0 15px
            border none 
            background-color rgba(0,0,0,0)
            color #ffffff
            font-size 20px
        .col-md-4
            margin 3% auto 3% auto
    .clock 
        margin 3% auto 3% auto
        button.time
            border 3px solid #ffffff
            border-radius 50%
            width 250px
            height 250px
            background-color rgba(0,0,0,0)
    .footer
        margin 60px auto auto auto
        a i  
            color #ffffff